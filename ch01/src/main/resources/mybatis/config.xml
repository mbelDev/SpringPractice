<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties resource="com/kwondev/mybatis/db.properties"></properties>
	<typeAliases>
		<typeAlias alias="BoardDto" type="com.kwondev.model.BoardDto" />
		<!-- <typeAlias alias="MemberDto" type="com.mbelDev.model.MemberDto" /> -->
		<!-- 별명 지정. com.mbelDev.model.BoardDto 라는 긴 이름을 BoardDto라고 퉁치겠다. b 라고 적어도 됨 ^^ -->
	</typeAliases>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${username}"/>
        <property name="password" value="${password}"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="com/kwondev/mybatis/BoardMapper.xml"/>
    <!-- <mapper resource="com/kwondev/mybatis/MemberMapper.xml"/> -->
    
  </mappers>
</configuration>